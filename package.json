{
  "name": "angular-material-datatransfer-app",
  "description": "A HTML5 datatransfer UI for handling upload and download of multiple simultaneous files.",
  "version": "4.0.4",
  "keywords": [
    "angular",
    "material-design",
    "uploader",
    "downloader",
    "datatransfer",
    "responsive",
    "web"
  ],
  "author": "https://github.com/niklr",
  "main": "dist/amd-app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niklr/angular-material-datatransfer.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-watch": "ng build --watch",
    "build-prod": "ng build --prod --output-hashing=none",
    "build-lib": "ng build amd-lib && node ./scripts/scss-bundle.build.js",
    "package-es5": "cd dist/amd-app && type runtime-es5.js polyfills-es5.js main-es5.js > angular-material-datatransfer-es5.js",
    "package-es2015": "cd dist/amd-app && type runtime-es2015.js polyfills-es2015.js main-es2015.js > angular-material-datatransfer-es2015.js",
    "append-banner": "node ./scripts/prepend.build.js",
    "bundle": "npm run build-prod amd-app && npm run package-es5 && npm run package-es2015 && npm run append-banner",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "gh-pages -d ./dist/amd-lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.13",
    "@angular/cdk": "^11.2.13",
    "@angular/material": "^11.2.13",
    "crypto-browserify": "git+ssh://git@github.com:niklr/crypto-browserify#7f681b3",
    "file-saver": "git+ssh://git@github.com:eligrey/FileSaver.js#5ed507e",
    "resumablejs": "git+ssh://git@github.com:chaos-sl/resumable.js.git#2d5e06b",
    "rxjs": "~6.6.3",
    "spark-md5": "^3.0.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular/cli": "~11.2.19",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "gh-pages": "^3.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.1.2",
    "protractor": "~7.0.0",
    "scss-bundle": "^3.0.2",
    "ts-node": "~8.5.4",
    "tslint": "~6.1.0",
    "typescript": "^4.1.3"
  }
}
